{% block block %}
<div class="{{ classes }}l-module block"{% if id %} id="{{ id }}"{% endif %}>
	<div class="l-inner block-inner">

		{% if primary_heading %}
			<h2 class="dark">{{ primary_heading }}</h2>
		{% endif %}

		{% if content %}
			<div class="block-description entry-content">{{ content }}</div>
		{% endif %}

	</div>
</div>
{% endblock %}
